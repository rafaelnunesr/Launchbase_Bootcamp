{% extends './layout.njk' %}

{% block header %}
    <title>Foodfy | Novo Chef</title>
{% endblock %}

{% block content %}

    <div class="field-component">
        <div class="field-title">
            <h1>Nome</h1>
        </div>

        <div class="field-input">
            <div class="field-chef-name">
                <input 
                    type="text">
            </div>
        </div>
    </div>

    <div class="field-component">
        <div class="field-title">
            <h1>Avatar</h1>
        </div>

        <div class="field-input">
            <div class="field-chef-avatar">
                <input 
                    type="text">
            </div>
        </div>
    </div>
{% endblock %}